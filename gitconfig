[alias]
  st = status
  co = checkout
  staged = diff --cached
  unstaged = diff
  both = diff HEAD

  # find a file in codebase
  f = "!git ls-files | grep -i"

  # list aliases
  la = "!git config -l | grep alias | cut -c 7-"
